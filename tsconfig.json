{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types", "./src"],
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    },
    "types": ["webpack-env"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
